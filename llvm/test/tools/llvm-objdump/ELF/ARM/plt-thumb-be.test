# RUN: yaml2obj %s -o %t
# RUN: llvm-objdump --no-print-imm-hex -d %t | FileCheck %s

# CHECK: Disassembly of section .text:
# CHECK: bl    {{.*}} <func1@plt>
# CHECK: bl    {{.*}} <func2@plt>
# CHECK: bl    {{.*}} <func3@plt>
# CHECK: b.w   {{.*}} <func1@plt>
# CHECK: b.w   {{.*}} <func2@plt>
# CHECK: b.w   {{.*}} <func3@plt>
# CHECK: beq.w {{.*}} <func1@plt>
# CHECK: beq.w {{.*}} <func2@plt>
# CHECK: beq.w {{.*}} <func3@plt>

# CHECK: Disassembly of section .plt:
# CHECK: <func1@plt>:
# CHECK: <func2@plt>:
# CHECK: <func3@plt>:

