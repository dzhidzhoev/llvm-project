# RUN: yaml2obj %s -o %t
# RUN: llvm-objdump -d %t | FileCheck %s

# CHECK: Disassembly of section .text:
# CHECK: b   {{.*}} <func1@plt>
# CHECK: bl  {{.*}} <func2@plt>
# CHECK: beq {{.*}} <func3@plt>

# CHECK: Disassembly of section .plt:
# CHECK: <func1@plt>:
# CHECK: <func2@plt>:
# CHECK: <func3@plt>:

# Test PLT section parsing on armeb

